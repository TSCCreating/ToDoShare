<div data-type="new">
  <div class="container">
    <div class="input-box">
      <%= simple_form_for(@todo) do |f| %>
        <div class="inputs">
          <%= f.error_notification %>
          <%= f.input :first_body %>
          <%= f.input :second_body %>
          <%= f.input :third_body %>
          <%= f.button :submit,'投稿！' %>
        </div>
      <% end %>
    </div>
  </div>
</div>
